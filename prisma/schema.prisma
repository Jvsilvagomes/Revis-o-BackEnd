generator client {
    provider = "prisma-client-js"
}

datasource db {
    provider = "postgresql"
}

model Song {
    id        Int      @id @default(autoincrement())
    title     String
    artist    String
    album     String?
    likes     Int      @default(0)
    createdAt DateTime @default(now())

    @@map("song")
}
